<div id="background_searches">
  <div id="general_events" class="users_container">
    <div id="main_div">

      <h2>General Event Search</h2>
      <div id="f1_container">
        <div id="f1_card" class="shadow">
          <div class="front face">
            <img src="/assets/events.jpg" alt="picture of nyc" title="picture of nyc" class="search_category_images"/>
          </div>
          <div class="back face center">
            <p><span class="event">Choose events by date.</span>
            </p>
          </div>
        </div>
      </div>
      <%= render partial: "search_form" %>

      <div id="plate" class="front">
            <textarea class="message">
            Dear User,
              Click this button for fun. :)
            Love, App Maker
            </textarea>
            <button class="send"> Press Me </button> <br/>
        </div>

        <div id="container" class="beginning">
            <div id="left-wing">
                <div class="top_left curvable"> </div>
                <div class="bottom_left curvable"> </div>
                <div class="wing wing1"></div>
                <div class="wing wing2"></div>
            </div>

            <div id="right-wing">
                <div class="top_right curvable"> </div>
                <div class="bottom_right curvable"> </div>
                <div class="wing wing3"></div>
                <div class="wing wing4"></div>
            </div>

        </div>

    </div>
  </div>
</div>
