/*CSS with jquery slidedown for 'description'*/
/* --------- COLLAPSIBLE PANELS ----------*/

.expandable-panel {
    width:100%;
    position:relative;
    min-height:50px;
    overflow:auto;
    margin-bottom: 20px;
    border:1px solid #999;
}
.expandable-panel-heading {
    width:100%;
    cursor:pointer;
    min-height:50px;
    clear:both;
    position:relative;
}
.expandable-panel-heading:hover h5 {
    color: #9A00EE;
}

.expandable-panel-heading h5 {
    padding:14px 10px 9px 20px;
    font-size:18px;
    line-height:20px;
    color: #00B6FF;
}
.expandable-panel-content {
    padding:0 15px 30px 15px;
    margin-top:-999px;
}
.expandable-panel-content p {
    padding: 4px 0 6px 0;
}

.expandable-panel-content p:first-child  {
    padding-top: 10px;
}
.expandable-panel-content p:last-child {
    padding-bottom:15px;
}
.icon-close-open {
    width:20px;
    height:20px;
    position:absolute;
    background-image:url(icon-close-open.png);
    right:15px;
}
.expandable-panel-content img {
    float:right;
    padding-left:12px;
}

